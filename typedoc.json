{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["packages/core/src/index.ts"],
  "out": "docs/api",
  "name": "Obora API Documentation",
  "readme": "README.md",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "includeVersion": true,
  "navigationLinks": {
    "GitHub": "https://github.com/de-novo/obora"
  },
  "plugin": [],
  "theme": "default",
  "categorizeByGroup": true,
  "categoryOrder": ["Engine", "Providers", "Tools", "Types", "*"],
  "sort": ["source-order"],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  }
}
